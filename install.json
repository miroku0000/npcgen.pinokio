{
  "run": [{
    "method": "shell.run",
    "params": { "message": "git clone https://github.com/miroku0000/npc-generator.git app" }
  }, {
    "method": "shell.run",
    "params": {
      "venv": "venv",
      "path": "app",
      "message": [
        "{{pip.install.torch}}",
        "pip install -r requirements.txt"
      ]
    }
  },
    {
    "method": "shell.run",
    "params": {
      "venv": "venv",
      "path": "app",
      "message": [
        "uncensoring",
        "fix.py"
      ]
    }
  },
    {
    "method": "input",
    "params": { "title": "Install Success", "description": "Go back to the dashboard and launch the app!" }
  }, {
    "method": "browser.open",
    "params": { "uri": "/?selected=npcgen" }
  }]
}
