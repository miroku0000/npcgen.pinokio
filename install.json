 {
  "run": [{
    "method": "shell.run",
    "params": { "message": "git clone https://github.com/miroku0000/npc-generator.git app" }
  }, {
    "method": "shell.run",
    "params": {
      "venv": "venv",
      "path": "app",
      "message": [
        "{{pip.install.torch}}",
        "pip install -r requirements.txt"
      ]
    }
  },
  {
    "method": "shell.run",
    "params": { "path":"app","message": "git clone https://github.com/miroku0000/moondream.git moondream" }
  },{
    "method": "shell.run",
    "params": {
      "venv": "moondream/venv",
      "path": "app/moondream",
      "message": [
        "{{pip.install.torch}}"
      ]
    }
  },
  {
    "method": "shell.run",
    "params": {
      "venv": "moondream/venv",
      "path": "app/",
      "message": [
        "{{pip.install.torch}}",
	"pwd",
        "pip install -r moondream/requirements.txt"
      ]
    }
  },
    {
    "method": "shell.run",
    "params": {
      "venv": "venv",
      "path": "app",
      "message": [
        "fix.py"
      ]
    }
  },
    {
    "method": "input",
    "params": { "title": "NPC Gen is now installed and uncensored", "description": "Go back to the dashboard and launch the app!" }
  }, {
    "method": "browser.open",
    "params": { "uri": "/?selected=npcgen" }
  }]
}
